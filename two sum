#include<iostream>
using namespace std;
int main(){
int n,i,j,target;
cout<<"enter the no. :\n";
cin>>n;
int a[n];
cout << "enter the numbers of the array \n:";
for(i=0;i<n;i++){
cin>>a[i];
}
cout<<"enter the taret :\n";
cin>>target;
for(i=0;i<n;i++){
for(j=0;j<n;j++){
if(a[i]+a[j]== target && i!=j){
cout<<"the indices are : ["<<i<<","<<j<<"]\n";}
}}
return 0;
}
